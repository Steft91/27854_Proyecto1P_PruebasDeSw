{
  "buildCommand": "npm run build",
  "outputDirectory": "dist/supermercado-frontend-angular/browser",
  "framework": null,
  "rewrites": [{ "source": "/(.*)", "destination": "/index.html" }],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [{ "key": "Cache-Control", "value": "no-cache" }]
    }
  ]
}
